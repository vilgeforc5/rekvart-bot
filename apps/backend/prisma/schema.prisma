generator client {
  provider     = "prisma-client"
  output       = "./generated"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Command {
  id          Int      @id @default(autoincrement())
  command     String   @unique
  title       String
  description String
  index       Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model StartContent {
  id        Int      @id @default(1)
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
